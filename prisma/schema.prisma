generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model Produto {
  id     Int     @id @default(autoincrement())
  nome   String
  preco_venda Float
  ncm       String
  unidade   String
}

